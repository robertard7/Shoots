+
+        return surfaceIds
+            .Where(id => !_registrations.ContainsKey(id))
+            .OrderBy(id => id, StringComparer.OrdinalIgnoreCase)
+            .ToList();
+    }
+
+    public IReadOnlyList<string> GetMissingSurfacePrefixes(IEnumerable<string> prefixes)
+    {
+        if (prefixes is null)
